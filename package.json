    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "mingcute_icon": "^2.7.1",
    "prettier": "^2.8.4",
    "primeicons": "^6.0.1",
    "primeng": "^15.2.0",
